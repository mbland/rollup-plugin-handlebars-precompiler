{
  "plugins": [
    "plugins/markdown",
    "jsdoc-plugin-typescript"
  ],
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+\\.js$",
    "exclude": ["node_modules"]
  },
  "typescript": {
    "moduleRoot": "."
  },
  "opts": {
    "destination": "jsdoc",
    "recurse": true,
    "readme": "README.md",
    "package": "package.json"
  }
}
